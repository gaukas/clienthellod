{      
    debug
    http_port    7080
	https_port   7443
    order clienthellod before file_server
    clienthellod {
        validfor 20s 30s
    }
    servers {
        listener_wrappers {
            clienthellod {
                tcp
                udp
            }
            tls
        }
    }
}

127.0.0.1, localhost {
    tls internal
    clienthellod
    file_server {
        root /var/www/html
    }
}